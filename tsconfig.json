		"./src/patch/Operation.ts",
		"./src/patch/Patch.ts",
		"./src/query/Filter.ts",
		"./src/query/Query.ts",
		"./src/query/Select.ts",
		"./src/query/Sort.ts",
		"./src/query/StoreRange.ts",
		"./src/store/MemoryStore.ts",
		"./src/store/RequestStore.ts",
		"./src/store/Store.ts",
		"./src/store/Transaction.ts",
		"./src/utils.ts",
		"./tests/intern-local.ts",
		"./tests/intern.ts",
		"./tests/unit/all.ts",
		"./tests/unit/store/filter.ts"
	]
}
